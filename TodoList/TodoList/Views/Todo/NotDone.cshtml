@model TodoViewModel

@{
    ViewData["Title"] = "NotDone";
}

<h1>NotDone</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Title</th>
            <th scope="col">Urgent</th>
            <th scope="col">Done</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var td in Model.Todo)
        {
            <tr>
                <th scope="row">@td.Id</th>
                <td>@td.Title</td>
                <td>@td.IsUrgent</td>
                <td>@td.IsDone</td>
            </tr>
        }



    </tbody>
</table>

